#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-cli@4.10.0_dqo7kfpsxuaie4a4pe35ztoh4i/node_modules/webpack-cli/bin/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-cli@4.10.0_dqo7kfpsxuaie4a4pe35ztoh4i/node_modules/webpack-cli/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-cli@4.10.0_dqo7kfpsxuaie4a4pe35ztoh4i/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-cli@4.10.0_dqo7kfpsxuaie4a4pe35ztoh4i/node_modules/webpack-cli/bin/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-cli@4.10.0_dqo7kfpsxuaie4a4pe35ztoh4i/node_modules/webpack-cli/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-cli@4.10.0_dqo7kfpsxuaie4a4pe35ztoh4i/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../common/temp/node_modules/.pnpm/webpack-cli@4.10.0_dqo7kfpsxuaie4a4pe35ztoh4i/node_modules/webpack-cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../common/temp/node_modules/.pnpm/webpack-cli@4.10.0_dqo7kfpsxuaie4a4pe35ztoh4i/node_modules/webpack-cli/bin/cli.js" "$@"
fi
