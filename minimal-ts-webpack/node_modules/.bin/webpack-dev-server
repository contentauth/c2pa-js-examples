#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-dev-server@4.9.2_hqh4vd6ku3d274yeebiz4wnr34/node_modules/webpack-dev-server/bin/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-dev-server@4.9.2_hqh4vd6ku3d274yeebiz4wnr34/node_modules/webpack-dev-server/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-dev-server@4.9.2_hqh4vd6ku3d274yeebiz4wnr34/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-dev-server@4.9.2_hqh4vd6ku3d274yeebiz4wnr34/node_modules/webpack-dev-server/bin/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-dev-server@4.9.2_hqh4vd6ku3d274yeebiz4wnr34/node_modules/webpack-dev-server/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/webpack-dev-server@4.9.2_hqh4vd6ku3d274yeebiz4wnr34/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../common/temp/node_modules/.pnpm/webpack-dev-server@4.9.2_hqh4vd6ku3d274yeebiz4wnr34/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../../common/temp/node_modules/.pnpm/webpack-dev-server@4.9.2_hqh4vd6ku3d274yeebiz4wnr34/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
