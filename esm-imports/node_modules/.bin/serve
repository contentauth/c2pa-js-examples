#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/serve@13.0.2/node_modules/serve/bin/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/serve@13.0.2/node_modules/serve/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/serve@13.0.2/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/node_modules:/home/runner/work/c2pa-js/c2pa-js/node_modules:/home/runner/work/c2pa-js/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/examples/esm-imports/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/examples/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/serve@13.0.2/node_modules/serve/bin/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/serve@13.0.2/node_modules/serve/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/serve@13.0.2/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules/.pnpm/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/temp/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/common/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/node_modules:/home/runner/work/c2pa-js/c2pa-js/node_modules:/home/runner/work/c2pa-js/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/examples/esm-imports/node_modules:/home/runner/work/c2pa-js/c2pa-js/workspace/examples/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../common/temp/node_modules/.pnpm/serve@13.0.2/node_modules/serve/bin/serve.js" "$@"
else
  exec node  "$basedir/../../../../common/temp/node_modules/.pnpm/serve@13.0.2/node_modules/serve/bin/serve.js" "$@"
fi
